{
  "name": "@lidofinance/lido-ethereum-sdk",
  "version": "0.0.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./common": {
      "import": "./dist/esm/common/index.js",
      "default": "./dist/cjs/common/index.js",
      "types": "./dist/types/common/index.d.ts"
    },
    "./core": {
      "import": "./dist/esm/core/index.js",
      "default": "./dist/cjs/core/index.js",
      "types": "./dist/types/core/index.d.ts"
    },
    "./staking": {
      "import": "./dist/esm/staking/index.js",
      "default": "./dist/cjs/staking/index.js",
      "types": "./dist/types/staking/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "common": [
        "./dist/types/common/index.d.ts"
      ],
      "core": [
        "./dist/types/core/index.d.ts"
      ],
      "staking": [
        "./dist/types/staking/index.d.ts"
      ]
    }
  },
  "keywords": [
    "lido",
    "lido-sdk",
    "lido-ethereum-sdk",
    "sdk",
    "ethereum",
    "stake",
    "withdrawals",
    "wrap"
  ],
  "homepage": "https://github.com/lidofinance/lido-ethereum-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/lidofinance/lido-ethereum-sdk.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/lidofinance/lido-ethereum-sdk/issues"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm && yarn build:types",
    "types": "tsc --noEmit",
    "publish:package": "yarn build && npm publish --registry http://localhost:4873/",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --outDir ./dist/cjs --removeComments --verbatimModuleSyntax false && echo > ./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --outDir ./dist/esm && echo > ./dist/esm/package.json '{\"type\":\"module\",\"sideEffects\":false}'",
    "build:types": "tsc --project tsconfig.build.json --module esnext --declarationDir ./dist/types --emitDeclarationOnly --declaration --declarationMap",
    "test": "echo test sdk",
    "prepublishOnly": "node scripts/updateVersion.cjs",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@lido-sdk/constants": "^3.1.0",
    "tiny-invariant": "^1.3.1",
    "viem": "^1.6.7"
  },
  "devDependencies": {
    "@lidofinance/eslint-config": "^0.35.0",
    "@types/fs-extra": "^11.0.1",
    "fs-extra": "^11.1.1",
    "rimraf": "^5.0.1",
    "typescript": "5.1.6"
  }
}
